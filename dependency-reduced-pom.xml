                <id>integration-test</id>
                <goals>
                  <goal>install</goal>
                  <goal>integration-test</goal>
                  <goal>verify</goal>
                </goals>
                <configuration>
                  <goals>
                    <goal>clean</goal>
                    <goal>process-sources</goal>
                  </goals>
                  <projectsDirectory>src/it/projects</projectsDirectory>
